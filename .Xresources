
! urxvt behelit color theme
URxvt.background:           #101010
URxvt.foreground:           #60ff60
URxvt.cursorColor:          #60ff60
URxvt.scrollColor:          #0a0a0a
URxvt.colorUL:              #5f87d7
URxvt.colorIT:              #d81860
URxvt.highlightColor:       #2368bb
URxvt.highlightTextColor:   #101010

URxvt.transparent: true
URxvt.shading: 10

! black
URxvt.color0:   #1c1c1c
URxvt.color8:   #4d4d4d
! red
URxvt.color1:   #d81860
URxvt.color9:   #f00060
! green
URxvt.color2:   #60ff60
URxvt.color10:  #70ff70
! yellow
URxvt.color3:   #f9fd75
URxvt.color11:  #f9fd80
! blue
URxvt.color4:   #5d97fa
URxvt.color12:  #679dfa
! magenta
URxvt.color5:   #a78edb
URxvt.color13:  #ad97db
! cyan
URxvt.color6:   #4695c8
URxvt.color14:  #5a9dc8
! white
URxvt.color7:   #f3ebe2
URxvt.color15:  #eeeeee

URxvt.font:                 xft:Source Code Pro:size=14,xft:Terminess\ Powerline
URxvt.termName:             rxvt-unicode-256color
URxvt.reverseVideo:         false
URxvt.scrollBar:            false
URxvt.saveLines:            10000
URxvt.cursorBlink:          true
URxvt.urgentOnBell:         true

! clickable urls
URxvt.perl-ext: default,matcher
URxvt.url-launcher: /usr/local/bin/xdg-open
URxvt.matcher.button: 1

! Copy/Paste
URxvt.perl-ext-common: selection-to-clipboard

