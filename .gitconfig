[user]
    name = Ricky Nelson
    email = rickyn@socketwiz.com
[core]
    excludesfile = /Users/socketwiz/.gitignore_global
[color]
    ui = true
[diff]
    tool = default-difftool
[merge]
    tool = default-difftool
[difftool "default-difftool"]
    cmd = default-difftool.sh $LOCAL $REMOTE
[difftool]
    prompt = false
[push]
    default = current
[init]
    templatedir = ~/.git_template
